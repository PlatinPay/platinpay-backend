{
  "name": "platinpay-backend",
  "module": "index.ts",
  "type": "module",
  "scripts": {
    "dev": "bun run --hot .",
    "start": "bun run .",
    "nuke-settings": "bun run nuke.ts settings",
    "nuke-main": "bun run nuke.ts main",
    "nuke": "bun run nuke-main && bun run nuke-settings",
    "populate": "bun run nuke-main && MODE=test bun run populate.ts",
    "genkey": "bun run nuke-settings && MODE=test bun run genkey.ts"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/uuid": "^10.0.0",
    "prettier": "^3.3.3"
  },
  "peerDependencies": {
    "typescript": "^5.6.2"
  },
  "dependencies": {
    "@iunstable0/server-libs": "^1.2.7",
    "axios": "^1.7.7",
    "canonicalize": "^2.0.0",
    "cors": "^2.8.5",
    "express": "^4.21.1",
    "uuid": "^10.0.0"
  }
}